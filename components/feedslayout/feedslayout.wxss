    height: 40rpx;
}

.feeds-item.ftype-note .feedinfo .fuser .fface .uface-box {
    width: 40rpx;
    height: 40rpx;
    position: relative;
}

.feeds-item.ftype-note .feedinfo .fuser .fface .uface-box .facepic {
    border-radius: 50%;
    width: 40rpx;
    height: 40rpx;
    background-color: #999;
    display: block;
}

.feeds-item.ftype-note .feedinfo .fuser .fface .uface-box .utype {
    position: absolute;
    bottom: -1rpx;
    right: -1rpx;
    width: 20rpx;
    height: 20rpx;
}

.feeds-item.ftype-note .feedinfo .fuser .fface .uface-box .utype.utype-2 {
    background: url("http://m.jiazhangbiji.cn/images/wxapp/daren-s@3x.png") 0 0 no-repeat;

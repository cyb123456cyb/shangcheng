}

.feedscontainer {
    padding: 0 20rpx;
}

.feedscontainer .feeds-l {
    margin: 0 20rpx 0 0;
}

.feedscontainer .feeds-l,.feedscontainer .feeds-r {
    display: inline-block;
    vertical-align: top;
}

.comm-feeds {
    margin: 0 20rpx;
    column-count: 2;
    column-gap: 0;
}

.comm-feeds .feeds-item {
    -webkit-column-break-inside: avoid;
    break-inside: avoid;
    display: inline-block;
}

.feeds-item {
    box-sizing: border-box;
    background: #ffffff;
    box-shadow: 0px 2px 12px 0px rgba(0,0,0,0.15);
    margin: 0 0 20rpx 0;
    border-radius: 12rpx;
    overflow: hidden;
}

.feeds-item.ftype-note {
    position: relative;
}

.feeds-item.ftype-note .toppic {
    position: relative;
    height: auto;
    overflow: hidden;
}

.feeds-item.ftype-note .toppic .feedpic {
    display: block;
}

.feeds-item.ftype-note .toppic .icon-hot {
    position: absolute;
    top: 0;
    left: 25rpx;
    width: 36rpx;
    height: 41rpx;
    background: url("http://m.jiazhangbiji.cn/images/wxapp/topicnote-hot.png?v=1") 0 0 no-repeat;
    background-size: 36rpx auto;
}

.feeds-item.ftype-note .toppic .icon-video {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 36rpx;
    height: 36rpx;
    background: url("http://m.jiazhangbiji.cn/images/wxapp/video-play.png") 0 0 no-repeat;
    background-size: 36rpx 36rpx;
}

.feeds-item.ftype-note .feedinfo {
    background-color: #fff;
    padding: 0 0 20rpx 0;
    position: relative;
}

.feeds-item.ftype-note .feedinfo .ftopic {
    padding: 28rpx 0 0 24rpx;
    height: 15rpx;
}

.feeds-item.ftype-note .feedinfo .ftopic .fttitle {
    display: inline-block;
    vertical-align: top;
    height: 15rpx;
    font-size: 22rpx;
    line-height: 15rpx;
    color: #000;
    padding: 0 6rpx 0 28rpx;
    background: url("http://m.jiazhangbiji.cn/images/wxapp/topic-icon.png") 6rpx 0 no-repeat;
    background-size: 17rpx auto;
    background-color: #FFEF16;
}

.feeds-item.ftype-note .feedinfo .fcontent {
    padding: 23rpx 24rpx 10rpx 24rpx;
    font-size: 26rpx;
    line-height: 37rpx;
}

.feeds-item.ftype-note .feedinfo .fcontent .ftsigndays {
    display: inline-block;
    vertical-align: top;
    height: 37rpx;
    padding: 0 7rpx;
    font-size: 26rpx;
    line-height: 37rpx;
    color: #FF1F5D;
    background-color: #FFD9E3;

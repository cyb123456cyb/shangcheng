    background-size: 30rpx 26rpx;
}

.feeds-item.ftype-note .feedinfo .flike .likecount {
    margin: 0 0 0 8rpx;
    display: inline-block;
    vertical-align: top;
    width: 30rpx;
    height: 30rpx;
    font-size: 22rpx;
    line-height: 30rpx;
    color: #808080;
}

.feeds-item.ftype-note .tsuserbg {
    position: absolute;
    z-index: 100;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    display: none;
}

.feeds-item.ftype-note .tsuserbg .tsactions {
    position: absolute;
    top: 50%;
    left: 42rpx;
    right: 42rpx;
    margin-top: -100rpx;
}

.feeds-item.ftype-note .tsuserbg .tsactions .btnact {
    margin: 60rpx 0 0 0;
    height: 70rpx;
    font-size: 28rpx;
    line-height: 70rpx;

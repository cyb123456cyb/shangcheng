    padding: 0 46rpx 0 0;
}

.feeds-item.ftype-note .tsuserbg .tsactions .btnact:first-child {
    margin-top: 0;
}

.feeds-item.ftype-note .tsuserbg .tsactions .btnact.btnaddhot {
    background: url("http://m.jiazhangbiji.cn/images/wxapp/suact-hot1.png") 44rpx 14rpx no-repeat;
    background-size: auto 41rpx;
    background-color: #fff;
}

.feeds-item.ftype-note .tsuserbg .tsactions .btnact.btncancelhot {
    background: url("http://m.jiazhangbiji.cn/images/wxapp/suact-hot2.png") 44rpx 14rpx no-repeat;
    background-size: auto 41rpx;
    background-color: #fff;
}

.feeds-item.ftype-note .tsuserbg .tsactions .btnact.btnhidenote {
    background: url("http://m.jiazhangbiji.cn/images/wxapp/suact-hide.png") 44rpx 14rpx no-repeat;

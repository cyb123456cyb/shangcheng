}

.feeds-item.ftype-note .feedinfo .fuser .fface .uface-box .utype.utype-3 {
    background: url("http://m.jiazhangbiji.cn/images/wxapp/xingma-s@3x.png") 0 0 no-repeat;
    background-size: auto 20rpx;
}

.feeds-item.ftype-note .feedinfo .fuser .fnick {
    margin: 0 0 0 50rpx;
    height: 40rpx;
    font-size: 26rpx;
    line-height: 40rpx;
    color: #403866;
    overflow: hidden;
}

.feeds-item.ftype-note .feedinfo .flike {
    position: absolute;
    bottom: 25rpx;
    right: 24rpx;
    height: 30rpx;
}

.feeds-item.ftype-note .feedinfo .flike .icon-like {
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    vertical-align: top;
    background: url("http://m.jiazhangbiji.cn/images/wxapp/icon-like.png") 0 2rpx no-repeat;
    background-size: 30rpx 26rpx;
}

.feeds-item.ftype-note .feedinfo .flike .icon-like.liked {


<view class="cu-custom" style="height:{{CustomBar}}px;">
    <view class="cu-bar fixed none-bg text-white bg-img" style="height:{{CustomBar}}px;padding-top:{{StatusBar}}px;">
        <view class="head-title canui-head-title">评价</view>
    </view>
</view>

<view class="service">
  <!--评分-->
  <view class='score'>
    <view class='left'>
      课程内容
    </view>
    <view class='right'>
       <view bindtap="starTap" data-index="{{index}}" data-level="{{0}}" class="star" wx:for="{{userStars[wjxScore[0]-1]}}" wx:key="index">
        <image src="{{item}}"></image>
      </view>
      <text class='rtxt'>{{wjxScore[0]}}.0分</text>
    </view>
  </view>
